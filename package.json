    "lib",
    "tasks",
    "src"
  ],
  "scripts": {
    "help": "plate help"
  },
  "dependencies": {
    "@idangoldman/plate": "link:",
    "coffeescript": "2.7.0"
  },
  "devDependencies": {
    "@coffeelint/cli": "5.2.11",
    "@cucumber/cucumber": "12.2.0",
    "@cucumber/pretty-formatter": "2.1.0",
    "chai": "6.0.1",
    "coffeescript": "2.7.0",
    "globals": "16.3.0",
    "lodash": "4.17.21",
    "prettier": "3.6.2",
    "yaml-types": "0.4.0",
    "yaml": "2.8.1"
  }
}
